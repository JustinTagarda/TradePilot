{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Connector": {
    "CloudApiBaseUrl": "http://localhost:5261",
    "SourceId": "connector-local"
  },
  "Security": {
    "InboundHmac": {
      "AllowedClockSkewSeconds": 300,
      "SharedSecret": "",
      "SourceSecrets": {
        "demo-source-01": "replace-with-ea-secret"
      }
    },
    "OutboundHmac": {
      "SharedSecret": "replace-with-cloud-api-secret",
      "SourceSecrets": {}
    }
  }
}
